import java.util.*;
import java.io.*;

public class Main {
  public static void main(String[] args) throws IOException {
    BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
    String what = br.readLine();
    
    for(int i = 0; i < what.length(); i++) {
      char check = what.charAt(i);
      if(Character.isLowerCase(check)) {
        System.out.print(Character.toUpperCase(check));
      } else {
        System.out.print(Character.toLowerCase(check));
      }
    }
    
    return ;
  }
}
